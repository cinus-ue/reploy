Target local

Task {
    # For loop with range
    For i 1 5 {
        Print "Iteration ${i}"
    }

    # For loop with step
    For j 10 0 -2 {
        Print "Countdown ${j}"
    }

    # Outer loop
    For i 1 3 {
        Print "Outer loop iteration ${i}"
        
        # Inner loop
        For j 1 2 {
            Print "  Inner loop iteration ${i}.${j}"
        }
    }
}
